<?xml version="1.0" encoding="utf-8" ?> 
<sqlMap namespace="CustomerProductsBom" xmlns="http://ibatis.apache.org/mapping" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <statements>
    <select id="selectBoms_by_product" resultMap="CustomerProductsBomResult">
        select * from CustomerProductsBom where primarykeyid = #value#
    </select>
    
    <delete id="delete_by_product" parameterClass="string">
      delete from CustomerProductsBom where primarykeyid = #value#
    </delete>
  </statements>  
</sqlMap>
			<result property="InvoiceCpbh" column="InvoiceCpbh" />
			<result property="InvoiceKslb" column="InvoiceKslb" />
			<result property="InvoiceKlfs" column="InvoiceKlfs" />
			<result property="InvoiceFpbh" column="InvoiceFpbh" />
			<result property="InvoiceKpls" column="InvoiceKpls" />
			<result property="InvoiceFpje" column="InvoiceFpje" />
			<result property="InvoiceCO" column="InvoiceCOId" lazyLoad="false" select="InvoiceCG.select_by_primary_key" />
			<result property="Supplier" column="SupplierId" lazyLoad="false" select="Supplier.select_by_primary_key" />
			<result property="Employee2" column="Employee2Id" lazyLoad="false" select="Employee.select_by_primary_key" />
			<result property="Employee0" column="Employee0Id" lazyLoad="false" select="Employee.select_by_primary_key" />
			<result property="Employee1" column="Employee1Id" lazyLoad="false" select="Employee.select_by_primary_key" />
			<result property="Employee3" column="Employee3Id" lazyLoad="false" select="Employee.select_by_primary_key" />
        </resultMap>
    </resultMaps>
		
    <statements>
		<select id="has_rows_of" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[InvoiceCT]
				WHERE
				[InvoiceId] = #InvoiceId#
				) then 1 else 0 end
		</select>
		<select id="has_rows" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[InvoiceCT]
 				where invoicestatus=1				
				) then 1 else 0 end
		</select>
		<select id="get_first" resultMap="InvoiceCTResult">
			<![CDATA[
			select top 1 *
			from dbo.InvoiceCT
			where invoicestatus=1
			order by InvoiceDate,inserttime
			]]>
		</select>
		<select id="get_last" resultMap="InvoiceCTResult">
			<![CDATA[
			select top 1 *
			from dbo.InvoiceCT
			where invoicestatus=1
			order by InvoiceDate desc,inserttime desc
			]]>
		</select>
		<select id="get_prev" resultMap="InvoiceCTResult" parameterClass="InvoiceCT">
			<![CDATA[
			select top 1 *
			from dbo.InvoiceCT
			where InvoiceDate < #InvoiceDate#
			order by InvoiceDate desc, inserttime desc
			]]>
		</select>
		<select id="get_next" resultMap="InvoiceCTResult" parameterClass="InvoiceCT">
			<![CDATA[
			select top 1 *
			from dbo.InvoiceCT
			where InvoiceDate > #InvoiceDate#
			order by InvoiceDate,inserttime
			]]>
		</select>
		
		<select id="has_rows_before" resultClass="bool" parameterClass="InvoiceCT">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.InvoiceCT
				where InvoiceDate < #InvoiceDate#
				) then 1 else 0 end
			]]>
		</select>
		
		<select id="has_rows_after" resultClass="bool" parameterClass="InvoiceCT">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.InvoiceCT
				where InvoiceDate > #InvoiceDate#
				) then 1 else 0 end
			]]>
		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="InvoiceCTResult" parameterClass="InvoiceCT">
			SELECT
				[InvoiceId],
				[Employee0Id],
				[SupplierId],
				[Employee1Id],
				[Employee2Id],
				[Employee3Id],
				[InvoiceCOId],
				[InvoiceLRTime],
				[InvoiceGZTime],
				[InvoiceZFTime],
				[InvoiceZFCause],
				[InvoiceDate],
				[InvoiceAbstract],
				[InvoiceNote],
				[InvoiceStatus],
				[InsertTime],
				[UpdateTime],
				[InvoiceHeJi],
				[InvoiceYHE],
				[InvoiceZSE],
				[InvoiceZRE],
				[InvoiceTax],
				[InvoiceZongJi],
				[InvoicePayTimeLimit],
				[InvoiceOwed],
				[InvoiceTaxRate],
				[InvoiceCpbh],
				[InvoiceKslb],
				[InvoiceKlfs],
				[InvoiceFpbh],
				[InvoiceKpls],
				[InvoiceFpje]
			FROM
				[dbo].[InvoiceCT]
			WHERE
				[InvoiceId] = #InvoiceId#
        </select>
		
		<!-- delete -->
        <delete id="delete" parameterClass="string">
			DELETE FROM [dbo].[InvoiceCT]
			WHERE
				[InvoiceId] = #InvoiceId#
        </delete>

		<!-- select all -->
        <select id="select_all" resultMap="InvoiceCTResult" >
			SELECT *
			FROM 
				[dbo].[InvoiceCT]
			ORDER BY InvoiceId ASC
        </select>
        
		<select id="count_all" resultClass="int">
			select count(*)
			from 
				[dbo].[InvoiceCT]
		</select>
		
		<select id="select_all_with_paging" resultMap="InvoiceCTResult">
            SELECT *
            FROM
            (
                SELECT *, row_number() over(order by $OrderStatement$) rownumber
                FROM    
                    [dbo].[InvoiceCT]
            ) t
            WHERE t.rownumber between #Row1# and #Row2#
		</select>
		
		<!-- insert -->
        <insert id="insert" parameterClass="InvoiceCT">
			INSERT INTO [dbo].[InvoiceCT] 
			(
				[InvoiceId],
				[Employee0Id],
				[SupplierId],
				[Employee1Id],
				[Employee2Id],
				[Employee3Id],
				[InvoiceCOId],
				[InvoiceLRTime],
				[InvoiceGZTime],
				[InvoiceZFTime],
				[InvoiceZFCause],
				[InvoiceDate],
				[InvoiceAbstract],
				[InvoiceNote],
				[InvoiceStatus],
				[InsertTime],
				[UpdateTime],
				[InvoiceHeJi],
				[InvoiceYHE],
				[InvoiceZSE],
				[InvoiceZRE],
				[InvoiceTax],
				[InvoiceZongJi],
				[InvoicePayTimeLimit],
				[InvoiceOwed],
				[InvoiceTaxRate],
				[InvoiceCpbh],
				[InvoiceKslb],
				[InvoiceKlfs],
				[InvoiceFpbh],
				[InvoiceKpls],
				[InvoiceFpje]
			) 
			VALUES 
			(
				#InvoiceId#,
				#Employee0Id#,
				#SupplierId#,
				#Employee1Id#,
				#Employee2Id#,
				#Employee3Id#,
				#InvoiceCOId#,
				#InvoiceLRTime#,
				#InvoiceGZTime#,
				#InvoiceZFTime#,
				#InvoiceZFCause#,
				#InvoiceDate#,
				#InvoiceAbstract#,
				#InvoiceNote#,
				#InvoiceStatus#,
				#InsertTime#,
				#UpdateTime#,
				#InvoiceHeJi#,
				#InvoiceYHE#,
				#InvoiceZSE#,
				#InvoiceZRE#,
				#InvoiceTax#,
				#InvoiceZongJi#,
				#InvoicePayTimeLimit#,
				#InvoiceOwed#,
				#InvoiceTaxRate#,
				#InvoiceCpbh#,
				#InvoiceKslb#,
				#InvoiceKlfs#,
				#InvoiceFpbh#,
				#InvoiceKpls#,
				#InvoiceFpje#
			)
        </insert>
		
		<!-- update -->
        <update id="update" parameterClass="InvoiceCT">
			UPDATE [InvoiceCT] SET
				[Employee0Id] = #Employee0Id#,
				[SupplierId] = #SupplierId#,
				[Employee1Id] = #Employee1Id#,
				[Employee2Id] = #Employee2Id#,
				[Employee3Id] = #Employee3Id#,
				[InvoiceCOId] = #InvoiceCOId#,
				[InvoiceLRTime] = #InvoiceLRTime#,
				[InvoiceGZTime] = #InvoiceGZTime#,
				[InvoiceZFTime] = #InvoiceZFTime#,
				[InvoiceZFCause] = #InvoiceZFCause#,
				[InvoiceDate] = #InvoiceDate#,
				[InvoiceAbstract] = #InvoiceAbstract#,
				[InvoiceNote] = #InvoiceNote#,
				[InvoiceStatus] = #InvoiceStatus#,
				[InsertTime] = #InsertTime#,
				[UpdateTime] = #UpdateTime#,
				[InvoiceHeJi] = #InvoiceHeJi#,
				[InvoiceYHE] = #InvoiceYHE#,
				[InvoiceZSE] = #InvoiceZSE#,
				[InvoiceZRE] = #InvoiceZRE#,
				[InvoiceTax] = #InvoiceTax#,
				[InvoiceZongJi] = #InvoiceZongJi#,
				[InvoicePayTimeLimit] = #InvoicePayTimeLimit#,
				[InvoiceOwed] = #InvoiceOwed#,
				[InvoiceTaxRate] = #InvoiceTaxRate#,
				[InvoiceCpbh] = #InvoiceCpbh#,
				[InvoiceKslb] = #InvoiceKslb#,
				[InvoiceKlfs] = #InvoiceKlfs#,
				[InvoiceFpbh] = #InvoiceFpbh#,
				[InvoiceKpls] = #InvoiceKpls#,
				[InvoiceFpje] = #InvoiceFpje#
			WHERE
				[InvoiceId] = #InvoiceId#
        </update>
		
		
    </statements>
</sqlMap>


<?xml version="1.0" encoding="utf-8" ?> 
<sqlMap namespace="Workhouselog" xmlns="http://ibatis.apache.org/mapping" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <alias>
        <typeAlias alias="Workhouselog" type="Book.Model.Workhouselog, Book.Model" />
    </alias>
    <resultMaps>                                    
        <resultMap id="WorkhouselogResult" class="Workhouselog">
			<result property="WorkhouselogID" column="WorkhouselogID" />
			<result property="WorkHouseId" column="WorkHouseId" />
			<result property="InsertTime" column="InsertTime" />
			<result property="UpdateTime" column="UpdateTime" />
			<result property="Workhouselogdate" column="Workhouselogdate" />
			<result property="Workhouselogcontent" column="Workhouselogcontent" />
			<result property="dd" column="dd" />
		<result property="WorkHouse" column="WorkHouseId" lazyLoad="false" select="WorkHouse.select_by_primary_key" />
        </resultMap>
    </resultMaps>
		
    <statements>
		<select id="has_rows_of" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[Workhouselog]
				WHERE
				[WorkhouselogID] = #WorkhouselogID#
				) then 1 else 0 end
		</select>
		<select id="has_rows" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[Workhouselog]
 								
				) then 1 else 0 end
		</select>
		<select id="get_first" resultMap="WorkhouselogResult">
			<![CDATA[
			select top 1 *
			from dbo.Workhouselog 
			order by InsertTime
			]]>
		</select>
		<select id="get_last" resultMap="WorkhouselogResult">
			<![CDATA[
			select top 1 *
			from dbo.Workhouselog 			
			order by InsertTime desc
			]]>
		</select>
		<select id="get_prev" resultMap="WorkhouselogResult" parameterClass="Workhouselog">
			<![CDATA[
			select top 1 *
			from dbo.Workhouselog
			where InsertTime < #InsertTime#
			order by InsertTime desc
			]]>
		</select>
		<select id="get_next" resultMap="WorkhouselogResult" parameterClass="Workhouselog">
			<![CDATA[
			select top 1 *
			from dbo.Workhouselog
			where InsertTime > #InsertTime#
			order by InsertTime
			]]>
		</select>
		<select id="has_rows_before" resultClass="bool" parameterClass="Workhouselog">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.Workhouselog
				where InsertTime < #InsertTime#
				) 
				then 1 else 0 end
			]]>
		</select>
		
		<select id="has_rows_after" resultClass="bool" parameterClass="Workhouselog">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.Workhouselog
				where InsertTime > #InsertTime#
				) then 1 else 0 end
			]]>
		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="WorkhouselogResult" parameterClass="Workhouselog">
			SELECT
				[WorkhouselogID],
				[WorkHouseId],
				[InsertTime],
				[UpdateTime],
				[Workhouselogdate],
				[Workhouselogcontent],
				[dd]
			FROM
				[dbo].[Workhouselog]
			WHERE
				[WorkhouselogID] = #WorkhouselogID#
        </select>
		
		<!-- delete -->
        <delete id="delete" parameterClass="string">
			DELETE FROM [dbo].[Workhouselog]
			WHERE
				[WorkhouselogID] = #WorkhouselogID#
        </delete>

		<!-- select all -->
        <select id="select_all" resultMap="WorkhouselogResult" >
			SELECT *
			FROM 
				[dbo].[Workhouselog]
			ORDER BY WorkhouselogID ASC
        </select>
        
		<select id="count_all" resultClass="int">
			select count(*)
			from 
				[dbo].[Workhouselog]
		</select>
		
		<select id="select_all_with_paging" resultMap="WorkhouselogResult">
            SELECT *
            FROM
            (
                SELECT *, row_number() over(order by $OrderStatement$) rownumber
                FROM    
                    [dbo].[Workhouselog]
            ) t
            WHERE t.rownumber between #Row1# and #Row2#
		</select>
		
		<!-- insert -->
        <insert id="insert" parameterClass="Workhouselog">
			INSERT INTO [dbo].[Workhouselog] 
			(
				[WorkhouselogID],
				[WorkHouseId],
				[InsertTime],
				[UpdateTime],
				[Workhouselogdate],
				[Workhouselogcontent],
				[dd]
			) 
			VALUES 
			(
				#WorkhouselogID#,
				#WorkHouseId#,
				#InsertTime#,
				#UpdateTime#,
				#Workhouselogdate#,
				#Workhouselogcontent#,
				#dd#
			)
        </insert>
		
		<!-- update -->
        <update id="update" parameterClass="Workhouselog">
			UPDATE [Workhouselog] SET
				[WorkHouseId] = #WorkHouseId#,
				[InsertTime] = #InsertTime#,
				[UpdateTime] = #UpdateTime#,
				[Workhouselogdate] = #Workhouselogdate#,
				[Workhouselogcontent] = #Workhouselogcontent#,
				[dd] = #dd#
			WHERE
				[WorkhouselogID] = #WorkhouselogID#
        </update>
		
		
    </statements>
</sqlMap>


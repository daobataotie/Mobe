<?xml version="1.0" encoding="utf-8" ?> 
<sqlMap namespace="CustomerProductProcess" xmlns="http://ibatis.apache.org/mapping" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <alias>
        <typeAlias alias="CustomerProductProcess" type="Book.Model.CustomerProductProcess, Book.Model" />
    </alias>
    <resultMaps>                                    
        <resultMap id="CustomerProductProcessResult" class="CustomerProductProcess">
			<result property="CustomerProductProcessId" column="CustomerProductProcessId" />
			<result property="ProcessCategoryId" column="ProcessCategoryId" />
			<result property="PrimaryKeyId" column="PrimaryKeyId" />
			<result property="ProcessId" column="ProcessId" />
			<result property="IsCheck" column="IsCheck" />
			<result property="PrimaryKey" column="PrimaryKeyId" lazyLoad="false" select="CustomerProducts.select_by_primary_key" />
			<result property="ProcessCategory" column="ProcessCategoryId" lazyLoad="false" select="ProcessCategory.select_by_primary_key" />
			<result property="Process" column="ProcessId" lazyLoad="false" select="Processing.select_by_primary_key" />
        </resultMap>
    </resultMaps>
		
    <statements>
		<select id="has_rows_of" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[CustomerProductProcess]
				WHERE
				[CustomerProductProcessId] = #CustomerProductProcessId#
				) then 1 else 0 end
		</select>
		<select id="has_rows" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[CustomerProductProcess]
 								
				) then 1 else 0 end
		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="CustomerProductProcessResult" parameterClass="CustomerProductProcess">
			SELECT
				[CustomerProductProcessId],
				[ProcessCategoryId],
				[PrimaryKeyId],
				[ProcessId],
				[IsCheck]
			FROM
				[dbo].[CustomerProductProcess]
			WHERE
				[CustomerProductProcessId] = #CustomerProductProcessId#
        </select>
		
		<!-- delete -->
        <delete id="delete" parameterClass="string">
			DELETE FROM [dbo].[CustomerProductProcess]
			WHERE
				[CustomerProductProcessId] = #CustomerProductProcessId#
        </delete>

		<!-- select all -->
        <select id="select_all" resultMap="CustomerProductProcessResult" >
			SELECT *
			FROM 
				[dbo].[CustomerProductProcess]
			ORDER BY CustomerProductProcessId ASC
        </select>
        
		<select id="count_all" resultClass="int">
			select count(*)
			from 
				[dbo].[CustomerProductProcess]
		</select>
		
		<select id="select_all_with_paging" resultMap="CustomerProductProcessResult">
            SELECT *
            FROM
            (
                SELECT *, row_number() over(order by $OrderStatement$) rownumber
                FROM    
                    [dbo].[CustomerProductProcess]
            ) t
            WHERE t.rownumber between #Row1# and #Row2#
		</select>
		
		<!-- insert -->
        <insert id="insert" parameterClass="CustomerProductProcess">
			INSERT INTO [dbo].[CustomerProductProcess] 
			(
				[CustomerProductProcessId],
				[ProcessCategoryId],
				[PrimaryKeyId],
				[ProcessId],
				[IsCheck]
			) 
			VALUES 
			(
				#CustomerProductProcessId#,
				#ProcessCategoryId#,
				#PrimaryKeyId#,
				#ProcessId#,
				#IsCheck#
			)
        </insert>
		
		<!-- update -->
        <update id="update" parameterClass="CustomerProductProcess">
			UPDATE [CustomerProductProcess] SET
				[ProcessCategoryId] = #ProcessCategoryId#,
				[PrimaryKeyId] = #PrimaryKeyId#,
				[ProcessId] = #ProcessId#,
				[IsCheck] = #IsCheck#
			WHERE
				[CustomerProductProcessId] = #CustomerProductProcessId#
        </update>
		
		
    </statements>
</sqlMap>


<?xml version="1.0" encoding="utf-8" ?> 
<sqlMap namespace="CustomerProcessingDetail" xmlns="http://ibatis.apache.org/mapping" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <alias>
        <typeAlias alias="CustomerProcessingDetail" type="Book.Model.CustomerProcessingDetail, Book.Model" />
    </alias>
    <resultMaps>                                    
        <resultMap id="CustomerProcessingDetailResult" class="CustomerProcessingDetail">
			<result property="CustomerProcessingDetailId" column="CustomerProcessingDetailId" />
			<result property="ProcessCategoryId" column="ProcessCategoryId" />
			<result property="ProcessId" column="ProcessId" />
			<result property="Description" column="Description" />
			<result property="CustomerProcessingId" column="CustomerProcessingId" />
		<result property="CustomerProcessing" column="CustomerProcessingId" lazyLoad="false" select="CustomerProcessing.select_by_primary_key" />		<result property="ProcessCategory" column="ProcessCategoryId" lazyLoad="false" select="ProcessCategory.select_by_primary_key" />		<result property="Process" column="ProcessId" lazyLoad="false" select="Processing.select_by_primary_key" />
        </resultMap>
    </resultMaps>
		
    <statements>
		<select id="has_rows_of" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[CustomerProcessingDetail]
				WHERE
				[CustomerProcessingDetailId] = #CustomerProcessingDetailId#
				) then 1 else 0 end
		</select>
		<select id="has_rows" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[CustomerProcessingDetail]
 								
				) then 1 else 0 end
		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="CustomerProcessingDetailResult" parameterClass="CustomerProcessingDetail">
			SELECT
				[CustomerProcessingDetailId],
				[ProcessCategoryId],
				[ProcessId],
				[Description],
				[CustomerProcessingId]
			FROM
				[dbo].[CustomerProcessingDetail]
			WHERE
				[CustomerProcessingDetailId] = #CustomerProcessingDetailId#
        </select>
		
		<!-- delete -->
        <delete id="delete" parameterClass="string">
			DELETE FROM [dbo].[CustomerProcessingDetail]
			WHERE
				[CustomerProcessingDetailId] = #CustomerProcessingDetailId#
        </delete>

		<!-- select all -->
        <select id="select_all" resultMap="CustomerProcessingDetailResult" >
			SELECT *
			FROM 
				[dbo].[CustomerProcessingDetail]
			
			
        </select>
        
		<select id="count_all" resultClass="int">
			select count(*)
			from 
				[dbo].[CustomerProcessingDetail]
		</select>
		
		<select id="select_all_with_paging" resultMap="CustomerProcessingDetailResult">
            SELECT *
            FROM
            (
                SELECT *, row_number() over(order by $OrderStatement$) rownumber
                FROM    
                    [dbo].[CustomerProcessingDetail]
            ) t
            WHERE t.rownumber between #Row1# and #Row2#
		</select>
		
		<!-- insert -->
        <insert id="insert" parameterClass="CustomerProcessingDetail">
			INSERT INTO [dbo].[CustomerProcessingDetail] 
			(
				[CustomerProcessingDetailId],
				[ProcessCategoryId],
				[ProcessId],
				[Description],
				[CustomerProcessingId]
			) 
			VALUES 
			(
				#CustomerProcessingDetailId#,
				#ProcessCategoryId#,
				#ProcessId#,
				#Description#,
				#CustomerProcessingId#
			)
        </insert>
		
		<!-- update -->
        <update id="update" parameterClass="CustomerProcessingDetail">
			UPDATE [CustomerProcessingDetail] SET
				[ProcessCategoryId] = #ProcessCategoryId#,
				[ProcessId] = #ProcessId#,
				[Description] = #Description#,
				[CustomerProcessingId] = #CustomerProcessingId#
			WHERE
				[CustomerProcessingDetailId] = #CustomerProcessingDetailId#
        </update>
		<select id="existsPrimary" resultClass="bool" parameterClass="string">
			<![CDATA[
			select case when exists(
				select *
				from [dbo].[CustomerProcessingDetail]
				where  CustomerProcessingDetailId = #value# 
				)
				then 1 else 0 end
			]]>
		</select>
		
    </statements>
</sqlMap>


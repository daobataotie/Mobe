    <statements>
		<select id="has_rows_of" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[ProduceTransfer]
				WHERE
				[ProduceTransferId] = #ProduceTransferId#
				) then 1 else 0 end
		</select>
		<select id="has_rows" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[ProduceTransfer]
 								
				) then 1 else 0 end
		</select>
		<select id="get_first" resultMap="ProduceTransferResult">
			<![CDATA[
			select top 1 *
			from dbo.ProduceTransfer 
			order by InsertTime
			]]>
		</select>
		<select id="get_last" resultMap="ProduceTransferResult">
			<![CDATA[
			select top 1 *
			from dbo.ProduceTransfer 			
			order by InsertTime desc
			]]>
		</select>
		<select id="get_prev" resultMap="ProduceTransferResult" parameterClass="ProduceTransfer">
			<![CDATA[
			select top 1 *
			from dbo.ProduceTransfer
			where InsertTime < #InsertTime#
			order by InsertTime desc
			]]>
		</select>
		<select id="get_next" resultMap="ProduceTransferResult" parameterClass="ProduceTransfer">
			<![CDATA[
			select top 1 *
			from dbo.ProduceTransfer
			where InsertTime > #InsertTime#
			order by InsertTime
			]]>
		</select>
		<select id="has_rows_before" resultClass="bool" parameterClass="ProduceTransfer">
			<![CDATA[
			select case when exists(
				select * 

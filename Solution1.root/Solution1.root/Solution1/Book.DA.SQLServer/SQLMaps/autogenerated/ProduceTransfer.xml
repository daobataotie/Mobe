				where InsertTime < #InsertTime#
				) 
				then 1 else 0 end
			]]>
		</select>
		
		<select id="has_rows_after" resultClass="bool" parameterClass="ProduceTransfer">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.ProduceTransfer
				where InsertTime > #InsertTime#
				) then 1 else 0 end
			]]>
		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="ProduceTransferResult" parameterClass="ProduceTransfer">
			SELECT

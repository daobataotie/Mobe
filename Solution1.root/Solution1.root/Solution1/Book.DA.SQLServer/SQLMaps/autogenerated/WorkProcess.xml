<?xml version="1.0" encoding="utf-8" ?> 
<sqlMap namespace="WorkProcess" xmlns="http://ibatis.apache.org/mapping" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <alias>
        <typeAlias alias="WorkProcess" type="Book.Model.WorkProcess, Book.Model" />
    </alias>
    <resultMaps>                                    
        <resultMap id="WorkProcessResult" class="WorkProcess">
			<result property="ProcessId" column="ProcessId" />
			<result property="InsertTime" column="InsertTime" />
			<result property="UpdateTime" column="UpdateTime" />
			<result property="ProcessDescription" column="ProcessDescription" />
			<result property="ProcessCenter" column="ProcessCenter" />
			<result property="ProcessCenterName" column="ProcessCenterName" />
			<result property="StartTime" column="StartTime" />
			<result property="EndTime" column="EndTime" />

        </resultMap>
    </resultMaps>
		
    <statements>
		<select id="has_rows_of" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[WorkProcess]
				WHERE
				[ProcessId] = #ProcessId#
				) then 1 else 0 end
		</select>
		<select id="has_rows" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[WorkProcess]
 								
				) then 1 else 0 end
		</select>
		<select id="get_first" resultMap="WorkProcessResult">
			<![CDATA[
			select top 1 *
			from dbo.WorkProcess 
			order by InsertTime
			]]>
		</select>
		<select id="get_last" resultMap="WorkProcessResult">
			<![CDATA[
			select top 1 *
			from dbo.WorkProcess 			
			order by InsertTime desc
			]]>
		</select>
		<select id="get_prev" resultMap="WorkProcessResult" parameterClass="WorkProcess">
			<![CDATA[
			select top 1 *
			from dbo.WorkProcess
			where InsertTime < #InsertTime#
			order by InsertTime desc
			]]>
		</select>
		<select id="get_next" resultMap="WorkProcessResult" parameterClass="WorkProcess">
			<![CDATA[
			select top 1 *
			from dbo.WorkProcess
			where InsertTime > #InsertTime#
			order by InsertTime
			]]>
		</select>
		<select id="has_rows_before" resultClass="bool" parameterClass="WorkProcess">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.WorkProcess
				where InsertTime < #InsertTime#
				) 
				then 1 else 0 end
			]]>
		</select>
		
		<select id="has_rows_after" resultClass="bool" parameterClass="WorkProcess">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.WorkProcess
				where InsertTime > #InsertTime#
				) then 1 else 0 end
			]]>
		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="WorkProcessResult" parameterClass="WorkProcess">
			SELECT
				[ProcessId],
				[InsertTime],
				[UpdateTime],
				[ProcessDescription],
				[ProcessCenter],
				[ProcessCenterName],
				[StartTime],
				[EndTime]
			FROM
				[dbo].[WorkProcess]
			WHERE
				[ProcessId] = #ProcessId#
        </select>
		
		<!-- delete -->
        <delete id="delete" parameterClass="string">
			DELETE FROM [dbo].[WorkProcess]
			WHERE
				[ProcessId] = #ProcessId#
        </delete>

		<!-- select all -->
        <select id="select_all" resultMap="WorkProcessResult" >
			SELECT *
			FROM 
				[dbo].[WorkProcess]
			ORDER BY ProcessId ASC
        </select>
        
		<select id="count_all" resultClass="int">
			select count(*)
			from 
				[dbo].[WorkProcess]
		</select>
		
		<select id="select_all_with_paging" resultMap="WorkProcessResult">
            SELECT *
            FROM
            (
                SELECT *, row_number() over(order by $OrderStatement$) rownumber
                FROM    
                    [dbo].[WorkProcess]
            ) t
            WHERE t.rownumber between #Row1# and #Row2#
		</select>
		
		<!-- insert -->
        <insert id="insert" parameterClass="WorkProcess">
			INSERT INTO [dbo].[WorkProcess] 
			(
				[ProcessId],
				[InsertTime],
				[UpdateTime],
				[ProcessDescription],
				[ProcessCenter],
				[ProcessCenterName],
				[StartTime],
				[EndTime]
			) 
			VALUES 
			(
				#ProcessId#,
				#InsertTime#,
				#UpdateTime#,
				#ProcessDescription#,
				#ProcessCenter#,
				#ProcessCenterName#,
				#StartTime#,
				#EndTime#
			)
        </insert>
		
		<!-- update -->
        <update id="update" parameterClass="WorkProcess">
			UPDATE [WorkProcess] SET
				[InsertTime] = #InsertTime#,
				[UpdateTime] = #UpdateTime#,
				[ProcessDescription] = #ProcessDescription#,
				[ProcessCenter] = #ProcessCenter#,
				[ProcessCenterName] = #ProcessCenterName#,
				[StartTime] = #StartTime#,
				[EndTime] = #EndTime#
			WHERE
				[ProcessId] = #ProcessId#
        </update>
		
		
    </statements>
</sqlMap>


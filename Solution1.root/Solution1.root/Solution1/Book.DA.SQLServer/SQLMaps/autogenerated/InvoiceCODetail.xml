		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="InvoiceCODetailResult" parameterClass="InvoiceCODetail">
			SELECT
				[InvoiceCODetailId],
				[ProductId],
				[InvoiceId],
				[OrderQuantity],
				[InvoiceCODetailPrice],
				[InvoiceCODetailMoney],

				#ProductId#,
				#InvoiceId#,
				#OrderQuantity#,
				#InvoiceCODetailPrice#,
				#InvoiceCODetailMoney#,
				#NoArrivalQuantity#,
				#ArrivalQuantity#,
				#InvoiceProductUnit#,
				#TotalMoney#,
				#InvoiceCODetailNote#,

<?xml version="1.0" encoding="utf-8" ?> 
<sqlMap namespace="DepotOut" xmlns="http://ibatis.apache.org/mapping" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <alias>
        <typeAlias alias="DepotOut" type="Book.Model.DepotOut, Book.Model" />
    </alias>
    <resultMaps>                                    
        <resultMap id="DepotOutResult" class="DepotOut">
			<result property="DepotOutId" column="DepotOutId" />
			<result property="EmployeeId" column="EmployeeId" />
			<result property="DepotId" column="DepotId" />
			<result property="InsertTime" column="InsertTime" />
			<result property="UpdateTime" column="UpdateTime" />
			<result property="DepotOutDate" column="DepotOutDate" />
			<result property="SourceType" column="SourceType" />
			<result property="description" column="description" />
			<result property="InvioiceId" column="InvioiceId" />
			<result property="ProductCategoryId" column="ProductCategoryId" />
		<result property="ProductCategory" column="ProductCategoryId" lazyLoad="false" select="ProductCategory.select_by_primary_key" />		<result property="Employee" column="EmployeeId" lazyLoad="false" select="Employee.select_by_primary_key" />		<result property="Depot" column="DepotId" lazyLoad="false" select="Depot.select_by_primary_key" />
        </resultMap>
    </resultMaps>
		
    <statements>
		<select id="has_rows_of" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[DepotOut]
				WHERE
				[DepotOutId] = #DepotOutId#
				) then 1 else 0 end
		</select>
		<select id="has_rows" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[DepotOut]
 								
				) then 1 else 0 end
		</select>
		<select id="get_first" resultMap="DepotOutResult">
			<![CDATA[
			select top 1 *
			from dbo.DepotOut 
			order by InsertTime
			]]>
		</select>
		<select id="get_last" resultMap="DepotOutResult">
			<![CDATA[
			select top 1 *
			from dbo.DepotOut 			
			order by InsertTime desc
			]]>
		</select>
		<select id="get_prev" resultMap="DepotOutResult" parameterClass="DepotOut">
			<![CDATA[
			select top 1 *
			from dbo.DepotOut
			where InsertTime < #InsertTime#
			order by InsertTime desc
			]]>
		</select>
		<select id="get_next" resultMap="DepotOutResult" parameterClass="DepotOut">
			<![CDATA[
			select top 1 *
			from dbo.DepotOut
			where InsertTime > #InsertTime#
			order by InsertTime
			]]>
		</select>
		<select id="has_rows_before" resultClass="bool" parameterClass="DepotOut">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.DepotOut
				where InsertTime < #InsertTime#
				) 
				then 1 else 0 end
			]]>
		</select>
		
		<select id="has_rows_after" resultClass="bool" parameterClass="DepotOut">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.DepotOut
				where InsertTime > #InsertTime#
				) then 1 else 0 end
			]]>
		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="DepotOutResult" parameterClass="DepotOut">
			SELECT
				[DepotOutId],
				[EmployeeId],
				[DepotId],
				[InsertTime],
				[UpdateTime],
				[DepotOutDate],
				[SourceType],
				[description],
				[InvioiceId],
				[ProductCategoryId]
			FROM
				[dbo].[DepotOut]
			WHERE
				[DepotOutId] = #DepotOutId#
        </select>
		
		<!-- delete -->
        <delete id="delete" parameterClass="string">
			DELETE FROM [dbo].[DepotOut]
			WHERE
				[DepotOutId] = #DepotOutId#
        </delete>

		<!-- select all -->
        <select id="select_all" resultMap="DepotOutResult" >
			SELECT *
			FROM 
				[dbo].[DepotOut]
			
			
        </select>
        
		<select id="count_all" resultClass="int">
			select count(*)
			from 
				[dbo].[DepotOut]
		</select>
		
		<select id="select_all_with_paging" resultMap="DepotOutResult">
            SELECT *
            FROM
            (
                SELECT *, row_number() over(order by $OrderStatement$) rownumber
                FROM    
                    [dbo].[DepotOut]
            ) t
            WHERE t.rownumber between #Row1# and #Row2#
		</select>
		
		<!-- insert -->
        <insert id="insert" parameterClass="DepotOut">
			INSERT INTO [dbo].[DepotOut] 
			(
				[DepotOutId],
				[EmployeeId],
				[DepotId],
				[InsertTime],
				[UpdateTime],
				[DepotOutDate],
				[SourceType],
				[description],
				[InvioiceId],
				[ProductCategoryId]
			) 
			VALUES 
			(
				#DepotOutId#,
				#EmployeeId#,
				#DepotId#,
				#InsertTime#,
				#UpdateTime#,
				#DepotOutDate#,
				#SourceType#,
				#description#,
				#InvioiceId#,
				#ProductCategoryId#
			)
        </insert>
		
		<!-- update -->
        <update id="update" parameterClass="DepotOut">
			UPDATE [DepotOut] SET
				[EmployeeId] = #EmployeeId#,
				[DepotId] = #DepotId#,
				[InsertTime] = #InsertTime#,
				[UpdateTime] = #UpdateTime#,
				[DepotOutDate] = #DepotOutDate#,
				[SourceType] = #SourceType#,
				[description] = #description#,
				[InvioiceId] = #InvioiceId#,
				[ProductCategoryId] = #ProductCategoryId#
			WHERE
				[DepotOutId] = #DepotOutId#
        </update>
		<select id="existsPrimary" resultClass="bool" parameterClass="string">
			<![CDATA[
			select case when exists(
				select *
				from [dbo].[DepotOut]
				where  DepotOutId = #value# 
				)
				then 1 else 0 end
			]]>
		</select>
		
    </statements>
</sqlMap>


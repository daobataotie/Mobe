<?xml version="1.0" encoding="utf-8" ?> 
<sqlMap namespace="InvoicePO" xmlns="http://ibatis.apache.org/mapping" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <alias>
        <typeAlias alias="InvoicePO" type="Book.Model.InvoicePO, Book.Model" />
    </alias>
    <resultMaps>                                    
        <resultMap id="InvoicePOResult" class="InvoicePO">
			<result property="InvoiceId" column="InvoiceId" />
			<result property="Employee0Id" column="Employee0Id" />
			<result property="DepartmentId" column="DepartmentId" />
			<result property="Employee1Id" column="Employee1Id" />
			<result property="Employee2Id" column="Employee2Id" />
			<result property="Employee3Id" column="Employee3Id" />
			<result property="InvoiceLRTime" column="InvoiceLRTime" />
			<result property="InvoiceGZTime" column="InvoiceGZTime" />
			<result property="InvoiceZFTime" column="InvoiceZFTime" />
			<result property="InvoiceZFCause" column="InvoiceZFCause" />
			<result property="InvoiceDate" column="InvoiceDate" />
			<result property="InvoiceAbstract" column="InvoiceAbstract" />
			<result property="InvoiceNote" column="InvoiceNote" />
			<result property="InvoiceStatus" column="InvoiceStatus" />
			<result property="InsertTime" column="InsertTime" />
			<result property="UpdateTime" column="UpdateTime" />
			<result property="InvoicePOQuantity" column="InvoicePOQuantity" />
			<result property="PoDepotId" column="PoDepotId" />
		<result property="PoDepot" column="PoDepotId" lazyLoad="false" select="Depot.select_by_primary_key" />		<result property="Department" column="DepartmentId" lazyLoad="false" select="Department.select_by_primary_key" />		<result property="Employee0" column="Employee0Id" lazyLoad="false" select="Employee.select_by_primary_key" />		<result property="Employee3" column="Employee3Id" lazyLoad="false" select="Employee.select_by_primary_key" />		<result property="Employee2" column="Employee2Id" lazyLoad="false" select="Employee.select_by_primary_key" />		<result property="Employee1" column="Employee1Id" lazyLoad="false" select="Employee.select_by_primary_key" />
        </resultMap>
    </resultMaps>
		
    <statements>
		<select id="has_rows_of" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[InvoicePO]
				WHERE
				[InvoiceId] = #InvoiceId#
				) then 1 else 0 end
		</select>
		<select id="has_rows" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[InvoicePO]
 				where invoicestatus=1				
				) then 1 else 0 end
		</select>
		<select id="get_first" resultMap="InvoicePOResult">
			<![CDATA[
			select top 1 *
			from dbo.InvoicePO
			where invoicestatus=1
			order by InvoiceDate,inserttime
			]]>
		</select>
		<select id="get_last" resultMap="InvoicePOResult">
			<![CDATA[
			select top 1 *
			from dbo.InvoicePO
			where invoicestatus=1
			order by InvoiceDate desc,inserttime desc
			]]>
		</select>
		<select id="get_prev" resultMap="InvoicePOResult" parameterClass="InvoicePO">
			<![CDATA[
			select top 1 *
			from dbo.InvoicePO
			where InvoiceDate < #InvoiceDate#
			order by InvoiceDate desc, inserttime desc
			]]>
		</select>
		<select id="get_next" resultMap="InvoicePOResult" parameterClass="InvoicePO">
			<![CDATA[
			select top 1 *
			from dbo.InvoicePO
			where InvoiceDate > #InvoiceDate#
			order by InvoiceDate,inserttime
			]]>
		</select>
		
		<select id="has_rows_before" resultClass="bool" parameterClass="InvoicePO">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.InvoicePO
				where InvoiceDate < #InvoiceDate#
				) then 1 else 0 end
			]]>
		</select>
		
		<select id="has_rows_after" resultClass="bool" parameterClass="InvoicePO">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.InvoicePO
				where InvoiceDate > #InvoiceDate#
				) then 1 else 0 end
			]]>
		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="InvoicePOResult" parameterClass="InvoicePO">
			SELECT
				[InvoiceId],
				[Employee0Id],
				[DepartmentId],
				[Employee1Id],
				[Employee2Id],
				[Employee3Id],
				[InvoiceLRTime],
				[InvoiceGZTime],
				[InvoiceZFTime],
				[InvoiceZFCause],
				[InvoiceDate],
				[InvoiceAbstract],
				[InvoiceNote],
				[InvoiceStatus],
				[InsertTime],
				[UpdateTime],
				[InvoicePOQuantity],
				[PoDepotId]
			FROM
				[dbo].[InvoicePO]
			WHERE
				[InvoiceId] = #InvoiceId#
        </select>
		
		<!-- delete -->
        <delete id="delete" parameterClass="string">
			DELETE FROM [dbo].[InvoicePO]
			WHERE
				[InvoiceId] = #InvoiceId#
        </delete>

		<!-- select all -->
        <select id="select_all" resultMap="InvoicePOResult" >
			SELECT *
			FROM 
				[dbo].[InvoicePO]
			
			
        </select>
        
		<select id="count_all" resultClass="int">
			select count(*)
			from 
				[dbo].[InvoicePO]
		</select>
		
		<select id="select_all_with_paging" resultMap="InvoicePOResult">
            SELECT *
            FROM
            (
                SELECT *, row_number() over(order by $OrderStatement$) rownumber
                FROM    
                    [dbo].[InvoicePO]
            ) t
            WHERE t.rownumber between #Row1# and #Row2#
		</select>
		
		<!-- insert -->
        <insert id="insert" parameterClass="InvoicePO">
			INSERT INTO [dbo].[InvoicePO] 
			(
				[InvoiceId],
				[Employee0Id],
				[DepartmentId],
				[Employee1Id],
				[Employee2Id],
				[Employee3Id],
				[InvoiceLRTime],
				[InvoiceGZTime],
				[InvoiceZFTime],
				[InvoiceZFCause],
				[InvoiceDate],
				[InvoiceAbstract],
				[InvoiceNote],
				[InvoiceStatus],
				[InsertTime],
				[UpdateTime],
				[InvoicePOQuantity],
				[PoDepotId]
			) 
			VALUES 
			(
				#InvoiceId#,
				#Employee0Id#,
				#DepartmentId#,
				#Employee1Id#,
				#Employee2Id#,
				#Employee3Id#,
				#InvoiceLRTime#,
				#InvoiceGZTime#,
				#InvoiceZFTime#,
				#InvoiceZFCause#,
				#InvoiceDate#,
				#InvoiceAbstract#,
				#InvoiceNote#,
				#InvoiceStatus#,
				#InsertTime#,
				#UpdateTime#,
				#InvoicePOQuantity#,
				#PoDepotId#
			)
        </insert>
		
		<!-- update -->
        <update id="update" parameterClass="InvoicePO">
			UPDATE [InvoicePO] SET
				[Employee0Id] = #Employee0Id#,
				[DepartmentId] = #DepartmentId#,
				[Employee1Id] = #Employee1Id#,
				[Employee2Id] = #Employee2Id#,
				[Employee3Id] = #Employee3Id#,
				[InvoiceLRTime] = #InvoiceLRTime#,
				[InvoiceGZTime] = #InvoiceGZTime#,
				[InvoiceZFTime] = #InvoiceZFTime#,
				[InvoiceZFCause] = #InvoiceZFCause#,
				[InvoiceDate] = #InvoiceDate#,
				[InvoiceAbstract] = #InvoiceAbstract#,
				[InvoiceNote] = #InvoiceNote#,
				[InvoiceStatus] = #InvoiceStatus#,
				[InsertTime] = #InsertTime#,
				[UpdateTime] = #UpdateTime#,
				[InvoicePOQuantity] = #InvoicePOQuantity#,
				[PoDepotId] = #PoDepotId#
			WHERE
				[InvoiceId] = #InvoiceId#
        </update>
		<select id="existsPrimary" resultClass="bool" parameterClass="string">
			<![CDATA[
			select case when exists(
				select *
				from [dbo].[InvoicePO]
				where  InvoiceId = #value# 
				)
				then 1 else 0 end
			]]>
		</select>
		
    </statements>
</sqlMap>


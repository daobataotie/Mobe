<?xml version="1.0" encoding="utf-8" ?> 
<sqlMap namespace="ManProcedure" xmlns="http://ibatis.apache.org/mapping" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <alias>
        <typeAlias alias="ManProcedure" type="Book.Model.ManProcedure, Book.Model" />
    </alias>
    <resultMaps>                                    
        <resultMap id="ManProcedureResult" class="ManProcedure">
			<result property="ManProcedureId" column="ManProcedureId" />
			<result property="BomId" column="BomId" />
			<result property="ProceduresId" column="ProceduresId" />
			<result property="TechonlogyHeaderId" column="TechonlogyHeaderId" />
			<result property="UpdateTime" column="UpdateTime" />
			<result property="CustomerId" column="CustomerId" />
			<result property="InsertTime" column="InsertTime" />
			<result property="MadeProductId" column="MadeProductId" />
		<result property="Customer" column="CustomerId" lazyLoad="false" select="Customer.select_by_primary_key" />		<result property="MadeProduct" column="MadeProductId" lazyLoad="false" select="Product.select_by_primary_key" />		<result property="Bom" column="BomId" lazyLoad="false" select="BomParentPartInfo.select_by_primary_key" />		<result property="Procedures" column="ProceduresId" lazyLoad="false" select="Procedures.select_by_primary_key" />		<result property="TechonlogyHeader" column="TechonlogyHeaderId" lazyLoad="false" select="TechonlogyHeader.select_by_primary_key" />
        </resultMap>
    </resultMaps>
		
    <statements>
		<select id="has_rows_of" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[ManProcedure]
				WHERE
				[ManProcedureId] = #ManProcedureId#
				) then 1 else 0 end
		</select>
		<select id="has_rows" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[ManProcedure]
 								
				) then 1 else 0 end
		</select>
		<select id="get_first" resultMap="ManProcedureResult">
			<![CDATA[
			select top 1 *
			from dbo.ManProcedure 
			order by InsertTime
			]]>
		</select>
		<select id="get_last" resultMap="ManProcedureResult">
			<![CDATA[
			select top 1 *
			from dbo.ManProcedure 			
			order by InsertTime desc
			]]>
		</select>
		<select id="get_prev" resultMap="ManProcedureResult" parameterClass="ManProcedure">
			<![CDATA[
			select top 1 *
			from dbo.ManProcedure
			where InsertTime < #InsertTime#
			order by InsertTime desc
			]]>
		</select>
		<select id="get_next" resultMap="ManProcedureResult" parameterClass="ManProcedure">
			<![CDATA[
			select top 1 *
			from dbo.ManProcedure
			where InsertTime > #InsertTime#
			order by InsertTime
			]]>
		</select>
		<select id="has_rows_before" resultClass="bool" parameterClass="ManProcedure">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.ManProcedure
				where InsertTime < #InsertTime#
				) 
				then 1 else 0 end
			]]>
		</select>
		
		<select id="has_rows_after" resultClass="bool" parameterClass="ManProcedure">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.ManProcedure
				where InsertTime > #InsertTime#
				) then 1 else 0 end
			]]>
		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="ManProcedureResult" parameterClass="ManProcedure">
			SELECT
				[ManProcedureId],
				[BomId],
				[ProceduresId],
				[TechonlogyHeaderId],
				[UpdateTime],
				[CustomerId],
				[InsertTime],
				[MadeProductId]
			FROM
				[dbo].[ManProcedure]
			WHERE
				[ManProcedureId] = #ManProcedureId#
        </select>
		
		<!-- delete -->
        <delete id="delete" parameterClass="string">
			DELETE FROM [dbo].[ManProcedure]
			WHERE
				[ManProcedureId] = #ManProcedureId#
        </delete>

		<!-- select all -->
        <select id="select_all" resultMap="ManProcedureResult" >
			SELECT *
			FROM 
				[dbo].[ManProcedure]
			
			
        </select>
        
		<select id="count_all" resultClass="int">
			select count(*)
			from 
				[dbo].[ManProcedure]
		</select>
		
		<select id="select_all_with_paging" resultMap="ManProcedureResult">
            SELECT *
            FROM
            (
                SELECT *, row_number() over(order by $OrderStatement$) rownumber
                FROM    
                    [dbo].[ManProcedure]
            ) t
            WHERE t.rownumber between #Row1# and #Row2#
		</select>
		
		<!-- insert -->
        <insert id="insert" parameterClass="ManProcedure">
			INSERT INTO [dbo].[ManProcedure] 
			(
				[ManProcedureId],
				[BomId],
				[ProceduresId],
				[TechonlogyHeaderId],
				[UpdateTime],
				[CustomerId],
				[InsertTime],
				[MadeProductId]
			) 
			VALUES 
			(
				#ManProcedureId#,
				#BomId#,
				#ProceduresId#,
				#TechonlogyHeaderId#,
				#UpdateTime#,
				#CustomerId#,
				#InsertTime#,
				#MadeProductId#
			)
        </insert>
		
		<!-- update -->
        <update id="update" parameterClass="ManProcedure">
			UPDATE [ManProcedure] SET
				[BomId] = #BomId#,
				[ProceduresId] = #ProceduresId#,
				[TechonlogyHeaderId] = #TechonlogyHeaderId#,
				[UpdateTime] = #UpdateTime#,
				[CustomerId] = #CustomerId#,
				[InsertTime] = #InsertTime#,
				[MadeProductId] = #MadeProductId#
			WHERE
				[ManProcedureId] = #ManProcedureId#
        </update>
		<select id="existsPrimary" resultClass="bool" parameterClass="string">
			<![CDATA[
			select case when exists(
				select *
				from [dbo].[ManProcedure]
				where  ManProcedureId = #value# 
				)
				then 1 else 0 end
			]]>
		</select>
		
    </statements>
</sqlMap>


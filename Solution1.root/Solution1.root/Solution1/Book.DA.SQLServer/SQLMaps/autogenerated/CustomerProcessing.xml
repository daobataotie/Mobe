<?xml version="1.0" encoding="utf-8" ?> 
<sqlMap namespace="CustomerProcessing" xmlns="http://ibatis.apache.org/mapping" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <alias>
        <typeAlias alias="CustomerProcessing" type="Book.Model.CustomerProcessing, Book.Model" />
    </alias>
    <resultMaps>                                    
        <resultMap id="CustomerProcessingResult" class="CustomerProcessing">
			<result property="CustomerProcessingId" column="CustomerProcessingId" />
			<result property="Id" column="Id" />
			<result property="CustomerProcessingDate" column="CustomerProcessingDate" />
			<result property="Directions" column="Directions" />
			<result property="CustomerId" column="CustomerId" />
			<result property="InsertTime" column="InsertTime" />
			<result property="UpdateTime" column="UpdateTime" />
			<result property="EmployeeId" column="EmployeeId" />
		<result property="Customer" column="CustomerId" lazyLoad="false" select="Customer.select_by_primary_key" />		<result property="Employee" column="EmployeeId" lazyLoad="false" select="Employee.select_by_primary_key" />
        </resultMap>
    </resultMaps>
		
    <statements>
		<select id="has_rows_of" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[CustomerProcessing]
				WHERE
				[CustomerProcessingId] = #CustomerProcessingId#
				) then 1 else 0 end
		</select>
		<select id="has_rows" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[CustomerProcessing]
 								
				) then 1 else 0 end
		</select>
		<select id="get_first" resultMap="CustomerProcessingResult">
			<![CDATA[
			select top 1 *
			from dbo.CustomerProcessing 
			order by InsertTime
			]]>
		</select>
		<select id="get_last" resultMap="CustomerProcessingResult">
			<![CDATA[
			select top 1 *
			from dbo.CustomerProcessing 			
			order by InsertTime desc
			]]>
		</select>
		<select id="get_prev" resultMap="CustomerProcessingResult" parameterClass="CustomerProcessing">
			<![CDATA[
			select top 1 *
			from dbo.CustomerProcessing
			where InsertTime < #InsertTime#
			order by InsertTime desc
			]]>
		</select>
		<select id="get_next" resultMap="CustomerProcessingResult" parameterClass="CustomerProcessing">
			<![CDATA[
			select top 1 *
			from dbo.CustomerProcessing
			where InsertTime > #InsertTime#
			order by InsertTime
			]]>
		</select>
		<select id="has_rows_before" resultClass="bool" parameterClass="CustomerProcessing">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.CustomerProcessing
				where InsertTime < #InsertTime#
				) 
				then 1 else 0 end
			]]>
		</select>
		
		<select id="has_rows_after" resultClass="bool" parameterClass="CustomerProcessing">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.CustomerProcessing
				where InsertTime > #InsertTime#
				) then 1 else 0 end
			]]>
		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="CustomerProcessingResult" parameterClass="CustomerProcessing">
			SELECT
				[CustomerProcessingId],
				[Id],
				[CustomerProcessingDate],
				[Directions],
				[CustomerId],
				[InsertTime],
				[UpdateTime],
				[EmployeeId]
			FROM
				[dbo].[CustomerProcessing]
			WHERE
				[CustomerProcessingId] = #CustomerProcessingId#
        </select>
		
		<!-- delete -->
        <delete id="delete" parameterClass="string">
			DELETE FROM [dbo].[CustomerProcessing]
			WHERE
				[CustomerProcessingId] = #CustomerProcessingId#
        </delete>

		<!-- select all -->
        <select id="select_all" resultMap="CustomerProcessingResult" >
			SELECT *
			FROM 
				[dbo].[CustomerProcessing]
			
				
			ORDER BY Id ASC
			
			
        </select>
        
		<select id="count_all" resultClass="int">
			select count(*)
			from 
				[dbo].[CustomerProcessing]
		</select>
		
		<select id="select_all_with_paging" resultMap="CustomerProcessingResult">
            SELECT *
            FROM
            (
                SELECT *, row_number() over(order by $OrderStatement$) rownumber
                FROM    
                    [dbo].[CustomerProcessing]
            ) t
            WHERE t.rownumber between #Row1# and #Row2#
		</select>
		
		<!-- insert -->
        <insert id="insert" parameterClass="CustomerProcessing">
			INSERT INTO [dbo].[CustomerProcessing] 
			(
				[CustomerProcessingId],
				[Id],
				[CustomerProcessingDate],
				[Directions],
				[CustomerId],
				[InsertTime],
				[UpdateTime],
				[EmployeeId]
			) 
			VALUES 
			(
				#CustomerProcessingId#,
				#Id#,
				#CustomerProcessingDate#,
				#Directions#,
				#CustomerId#,
				#InsertTime#,
				#UpdateTime#,
				#EmployeeId#
			)
        </insert>
		
		<!-- update -->
        <update id="update" parameterClass="CustomerProcessing">
			UPDATE [CustomerProcessing] SET
				[Id] = #Id#,
				[CustomerProcessingDate] = #CustomerProcessingDate#,
				[Directions] = #Directions#,
				[CustomerId] = #CustomerId#,
				[InsertTime] = #InsertTime#,
				[UpdateTime] = #UpdateTime#,
				[EmployeeId] = #EmployeeId#
			WHERE
				[CustomerProcessingId] = #CustomerProcessingId#
        </update>
		<select id="existsPrimary" resultClass="bool" parameterClass="string">
			<![CDATA[
			select case when exists(
				select *
				from [dbo].[CustomerProcessing]
				where  CustomerProcessingId = #value# 
				)
				then 1 else 0 end
			]]>
		</select>
		<select id="exists" resultClass="bool" parameterClass="string">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.CustomerProcessing
				where Id = #value#
				) then 1 else 0 end
			]]>
		</select>
		
		<select id="get_by_id" resultMap="CustomerProcessingResult" parameterClass="string">
			<![CDATA[
			SELECT top 1
				*
			FROM
				[dbo].[CustomerProcessing]
			WHERE
				Id = #value#
			]]>	
        </select>
		
		<select id="existsexcept" resultClass="bool" parameterClass="string">
			<![CDATA[
			select case when exists(
				select *
				from dbo.CustomerProcessing
				where Id = #newId# and Id <> #oldId#
				)
				then 1 else 0 end
			]]>
		</select>		
		
		
    </statements>
</sqlMap>


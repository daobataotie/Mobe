<?xml version="1.0" encoding="utf-8" ?> 
<sqlMap namespace="PackageType" xmlns="http://ibatis.apache.org/mapping" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <alias>
        <typeAlias alias="PackageType" type="Book.Model.PackageType, Book.Model" />
    </alias>
    <resultMaps>                                    
        <resultMap id="PackageTypeResult" class="PackageType">
			<result property="Id" column="Id" />
			<result property="PackageTypeId" column="PackageTypeId" />
			<result property="InsertTime" column="InsertTime" />
			<result property="UpdateTime" column="UpdateTime" />
			<result property="PackagePypeName" column="PackagePypeName" />
			<result property="CreateDate" column="CreateDate" />
			<result property="Description" column="Description" />
        </resultMap>
    </resultMaps>
		
    <statements>
		<select id="has_rows_of" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[PackageType]
				WHERE
				[PackageTypeId] = #PackageTypeId#
				) then 1 else 0 end
		</select>
		<select id="has_rows" resultClass="bool">
			select case when exists(
				SELECT
					*
				FROM
					[dbo].[PackageType]
 								
				) then 1 else 0 end
		</select>
		<select id="get_first" resultMap="PackageTypeResult">
			<![CDATA[
			select top 1 *
			from dbo.PackageType 
			order by InsertTime
			]]>
		</select>
		<select id="get_last" resultMap="PackageTypeResult">
			<![CDATA[
			select top 1 *
			from dbo.PackageType 			
			order by InsertTime desc
			]]>
		</select>
		<select id="get_prev" resultMap="PackageTypeResult" parameterClass="PackageType">
			<![CDATA[
			select top 1 *
			from dbo.PackageType
			where InsertTime < #InsertTime#
			order by InsertTime desc
			]]>
		</select>
		<select id="get_next" resultMap="PackageTypeResult" parameterClass="PackageType">
			<![CDATA[
			select top 1 *
			from dbo.PackageType
			where InsertTime > #InsertTime#
			order by InsertTime
			]]>
		</select>
		<select id="has_rows_before" resultClass="bool" parameterClass="PackageType">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.PackageType
				where InsertTime < #InsertTime#
				) 
				then 1 else 0 end
			]]>
		</select>
		
		<select id="has_rows_after" resultClass="bool" parameterClass="PackageType">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.PackageType
				where InsertTime > #InsertTime#
				) then 1 else 0 end
			]]>
		</select>
		<!-- select by primary key -->
        <select id="select_by_primary_key" resultMap="PackageTypeResult" parameterClass="PackageType">
			SELECT
				[Id],
				[PackageTypeId],
				[InsertTime],
				[UpdateTime],
				[PackagePypeName],
				[CreateDate],
				[Description]
			FROM
				[dbo].[PackageType]
			WHERE
				[PackageTypeId] = #PackageTypeId#
        </select>
		
		<!-- delete -->
        <delete id="delete" parameterClass="string">
			DELETE FROM [dbo].[PackageType]
			WHERE
				[PackageTypeId] = #PackageTypeId#
        </delete>

		<!-- select all -->
        <select id="select_all" resultMap="PackageTypeResult" >
			SELECT *
			FROM 
				[dbo].[PackageType]
			ORDER BY PackageTypeId ASC
        </select>
        
		<select id="count_all" resultClass="int">
			select count(*)
			from 
				[dbo].[PackageType]
		</select>
		
		<select id="select_all_with_paging" resultMap="PackageTypeResult">
            SELECT *
            FROM
            (
                SELECT *, row_number() over(order by $OrderStatement$) rownumber
                FROM    
                    [dbo].[PackageType]
            ) t
            WHERE t.rownumber between #Row1# and #Row2#
		</select>
		
		<!-- insert -->
        <insert id="insert" parameterClass="PackageType">
			INSERT INTO [dbo].[PackageType] 
			(
				[Id],
				[PackageTypeId],
				[InsertTime],
				[UpdateTime],
				[PackagePypeName],
				[CreateDate],
				[Description]
			) 
			VALUES 
			(
				#Id#,
				#PackageTypeId#,
				#InsertTime#,
				#UpdateTime#,
				#PackagePypeName#,
				#CreateDate#,
				#Description#
			)
        </insert>
		
		<!-- update -->
        <update id="update" parameterClass="PackageType">
			UPDATE [PackageType] SET
				[Id] = #Id#,
				[InsertTime] = #InsertTime#,
				[UpdateTime] = #UpdateTime#,
				[PackagePypeName] = #PackagePypeName#,
				[CreateDate] = #CreateDate#,
				[Description] = #Description#
			WHERE
				[PackageTypeId] = #PackageTypeId#
        </update>
		
		<select id="exists" resultClass="bool" parameterClass="string">
			<![CDATA[
			select case when exists(
				select * 
				from dbo.PackageType
				where Id = #value#
				) then 1 else 0 end
			]]>
		</select>
		
		<select id="get_by_id" resultMap="PackageTypeResult" parameterClass="string">
			<![CDATA[
			SELECT top 1
				*
			FROM
				[dbo].[PackageType]
			WHERE
				Id = #value#
			]]>	
        </select>
		
		<select id="existsexcept" resultClass="bool">
			<![CDATA[
			select case when exists(
				select *
				from dbo.PackageType
				where Id = #newId# and Id <> #oldId#
				)
				then 1 else 0 end
			]]>
		</select>
		
    </statements>
</sqlMap>


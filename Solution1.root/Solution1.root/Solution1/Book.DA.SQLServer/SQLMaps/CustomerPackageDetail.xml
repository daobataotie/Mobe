<?xml version="1.0" encoding="utf-8" ?> 
<sqlMap namespace="CustomerPackageDetail" xmlns="http://ibatis.apache.org/mapping" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<statements>  
  <select id="select_byPackageId" resultMap="CustomerPackageDetailResult" >
    SELECT *
    FROM
    [CustomerPackageDetail]
    WHERE
    [CustomerPackageId] = #value#  
  </select>
          
</statements>
</sqlMap>
    <select id="existsPrimary" resultClass="bool" parameterClass="string">
      <![CDATA[
			select case when exists(
				select *
				from [dbo].[PCOtherCheck]
				where  PCOtherCheckId = #value# 
				)
				then 1 else 0 end
			]]>
    </select>

  </statements>

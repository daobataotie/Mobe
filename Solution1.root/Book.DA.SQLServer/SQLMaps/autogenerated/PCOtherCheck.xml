      #PCOtherCheckId#,
      #EmployeeId#,
      #SupplierId#,
      #Employee0Id#,
      #Employee1Id#,
      #InsertTime#,
      #UpdateTime#,
      #PCOtherCheckDate#,
      #PCOtherCheckDesc#,
      #InvoiceCusXOId#,
      #FromPCType#,
      #AuditState#,
      #AuditEmpId#,
      #CBEmployeeId#
      )

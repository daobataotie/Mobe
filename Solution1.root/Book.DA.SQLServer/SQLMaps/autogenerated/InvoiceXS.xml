  <statements>
    <select id="has_rows_of" resultClass="bool">
      <![CDATA[select case when exists(
				SELECT
					*
				FROM
					[dbo].[InvoiceXS]
				WHERE
				[InvoiceId] = #InvoiceId#
                         and invoicestatus<>2 			                   
				) then 1 else 0 end
                ]]>
    </select>
    <select id="has_rows" resultClass="bool">
      <![CDATA[select case when exists(
				SELECT
					*
				FROM
					[dbo].[InvoiceXS]
 				where invoicestatus<>2 				
				) then 1 else 0 end
                ]]>
    </select>
    <select id="get_first" resultMap="InvoiceXSResult">
      <![CDATA[
			select top 1 *
			from dbo.InvoiceXS
			where invoicestatus<>2 
			order by InsertTime 
			]]>
    </select>
    <select id="get_last" resultMap="InvoiceXSResult">
      <![CDATA[
			select top 1 *
			from dbo.InvoiceXS
			where invoicestatus<>2			
            order by InsertTime desc
			]]>

    </select>
    <select id="get_prev" resultMap="InvoiceXSResult" parameterClass="InvoiceXS">
      <![CDATA[
			select top 1 *
			from dbo.InvoiceXS
			where InsertTime < #InsertTime#	
             and invoicestatus<>2 			          
            order by InsertTime desc
			]]>
    </select>
    <select id="get_next" resultMap="InvoiceXSResult" parameterClass="InvoiceXS">
      <![CDATA[
			select top 1 *
			from dbo.InvoiceXS
			where InsertTime > #InsertTime#	
                 and invoicestatus<>2			                   
            order by InsertTime
			]]>
    </select>

    <select id="has_rows_before" resultClass="bool" parameterClass="InvoiceXS">
      <![CDATA[
			select case when exists(
				select * 
				from dbo.InvoiceXS
				where InsertTime < #InsertTime#
                 and invoicestatus<>2			                   
				) then 1 else 0 end
			]]>
    </select>

    <select id="has_rows_after" resultClass="bool" parameterClass="InvoiceXS">
      <![CDATA[
			select case when exists(
				select * 
				from dbo.InvoiceXS
				where InsertTime > #InsertTime#
                 and invoicestatus<>2 			          
         
				) then 1 else 0 end
			]]>
    </select>
    <!-- select by primary key -->
    <select id="select_by_primary_key" resultMap="InvoiceXSResult" parameterClass="InvoiceXS">
      <![CDATA[SELECT
				[InvoiceId],
				[CustomerId],
				[Employee0Id],
				[DepotId],
				[Employee1Id],
				[Employee2Id],
				[Employee3Id],
				[InvoiceXOId],
				[InvoiceLRTime],
				[InvoiceGZTime],
				[InvoiceZFTime],
				[InvoiceZFCause],
				[InvoiceDate],
				[InvoiceAbstract],
				[InvoiceNote],
				[InvoiceStatus],
				[InsertTime],
				[UpdateTime],
				[TransportCompany],
				[ProductType],
				[XSCustomerId],
				[ConveyanceMethodId],
				[InvoiceTotal],
				[InvoiceTax],
				[InvoiceHeji],
				[InvoiceTaxrate],
				[TaxCaluType],
				[InvoiceAllowance],
				[AuditState],
				[AuditEmpId],
				[OtherChargeMoney],
				[OtherChargeNote]
			FROM
				[dbo].[InvoiceXS]
			WHERE
				[InvoiceId] = #InvoiceId#
                 and invoicestatus<>2 			          
         		]]>
    </select>

    <!-- delete -->
    <delete id="delete" parameterClass="string">
      <![CDATA[                
                DELETE FROM [dbo].[InvoiceXS]
			WHERE            
				[InvoiceId] = #InvoiceId#
                    		]]>
    </delete>

    <!-- select all -->
    <select id="select_all" resultMap="InvoiceXSResult" >
      <![CDATA[SELECT *
			FROM 
				[dbo].[InvoiceXS]
                 where  invoicestatus<>2 			                   
  ORDER BY InsertTime DESC  
			]]>
    </select>

    <select id="count_all" resultClass="int">
      <![CDATA[	select count(*)
			from 
				[dbo].[InvoiceXS]
                 where invoicestatus<>2 			          
         ]]>
    </select>

    <select id="select_all_with_paging" resultMap="InvoiceXSResult">
      <![CDATA[    SELECT *
            FROM
            (
                SELECT *, row_number() over(order by $OrderStatement$) rownumber
                FROM    
                    [dbo].[InvoiceXS]
                     where  invoicestatus<>2 			          
         
            ) t
            WHERE t.rownumber between #Row1# and #Row2#
             ]]>
    </select>

    <!-- insert -->
    <insert id="insert" parameterClass="InvoiceXS">
      <![CDATA[ INSERT INTO [dbo].[InvoiceXS] 

			]]>
    </select>
    <select id="get_next" resultMap="PCMouldOnlineCheckResult" parameterClass="PCMouldOnlineCheck">
      <![CDATA[
			select top 1 *
			from dbo.PCMouldOnlineCheck
			where InsertTime > #InsertTime#	
                			                   

		</select>
		<select id="get_first" resultMap="SalesForHeaderResult">
			<![CDATA[
			select top 1 *
			from dbo.SalesForHeader 
			order by InsertTime
			]]>
		</select>
		<select id="get_last" resultMap="SalesForHeaderResult">
			<![CDATA[
			select top 1 *
			from dbo.SalesForHeader 			

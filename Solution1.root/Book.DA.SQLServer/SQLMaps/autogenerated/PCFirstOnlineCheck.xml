    <select id="get_prev" resultMap="PCFirstOnlineCheckResult" parameterClass="PCFirstOnlineCheck">
      <![CDATA[
			select top 1 *
			from dbo.PCFirstOnlineCheck
			where InsertTime < #InsertTime#	
            			          

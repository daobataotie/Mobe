                    		]]>
    </delete>

    <!-- select all -->
    <select id="select_all" resultMap="InvoiceZXDetailResult" >
      <![CDATA[SELECT *
			FROM 
				[dbo].[InvoiceZXDetail]
                			                   

			]]>
    </select>

    <select id="count_all" resultClass="int">

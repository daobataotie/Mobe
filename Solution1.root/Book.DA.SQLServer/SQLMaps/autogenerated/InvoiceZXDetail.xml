      <![CDATA[select case when exists(
				SELECT
					*
				FROM
					[dbo].[InvoiceZXDetail]
				WHERE
				[InvoiceZXDetailId] = #InvoiceZXDetailId#
                        			                   
				) then 1 else 0 end
                ]]>

      [InvoiceId],
      [InvoiceCJDetailPrice],
      [InvoiceCJDetailQuantity],
      [InvoiceCJDetailMoney],
      [InvoiceCJDetailNote],
      [InvoiceProductUnit],
      [Inumber],

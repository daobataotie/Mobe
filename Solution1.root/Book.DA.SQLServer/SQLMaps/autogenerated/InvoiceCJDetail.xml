      #ProductId#,
      #InvoiceId#,
      #InvoiceCJDetailPrice#,
      #InvoiceCJDetailQuantity#,
      #InvoiceCJDetailMoney#,
      #InvoiceCJDetailNote#,
      #InvoiceProductUnit#,
      #Inumber#,
      #HandbookId#,

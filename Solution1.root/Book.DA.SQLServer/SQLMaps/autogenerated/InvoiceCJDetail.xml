      [ProductId] = #ProductId#,
      [InvoiceId] = #InvoiceId#,
      [InvoiceCJDetailPrice] = #InvoiceCJDetailPrice#,
      [InvoiceCJDetailQuantity] = #InvoiceCJDetailQuantity#,

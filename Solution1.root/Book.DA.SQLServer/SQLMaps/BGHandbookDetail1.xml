    <select id="SelectBGProduct" resultMap="BGHandbookDetail1Result" parameterClass="Map">
      <![CDATA[
      SELECT TOP 1 * FROM BGHandbookDetail1 WHERE Proname IS NOT NULL $sql$
      ]]>
    </select>
